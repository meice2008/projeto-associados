@model IEnumerable<AssociadoModel>

<div class="container">

    <div class="d-flex justify-content-between align-items-center mb-5">
       
        <h1>Associados</h1>

        <a class="btn btn-success mx-2">Cadastrar</a>

    </div>

    <table class="table table-dark table-hover">

        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Id</th>
                <th scope="col">Nome</th>
                <th scope="col">Cpf</th>
                <th scope="col">Data de Nascimento</th>
            </tr>
        </thead>

        <tbody>

            @if (Model != null)
            {
                @foreach (var associado in Model)
                {
                    <tr>
                        <th scope="row"></th>
                        <td>@associado.Id</td>
                        <td>@associado.Nome</td>
                        <td>@associado.Cpf</td>
                        <td>@associado.DtNascimento</td>
                    </tr>
                }
            }


        </tbody>
    </table>

</div>

